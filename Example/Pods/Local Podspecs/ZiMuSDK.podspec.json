  "resource_bundles": {
    "ZiMuSDK": [
      "ZiMuSDK/Assets/*.png"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
