        {
          "name": "ZiMuCommon",
          "source_files": "ZiMuSDK/Classes/ZiMuCommon/**/*.{h,m}"
        },
        {
          "name": "ZiMuPay",
          "source_files": "ZiMuSDK/Classes/ZiMuPay/**/*.{h,m}",
          "dependencies": {
            "WechatOpenSDK": [

            ],
            "AliPay": [

            ]
          }
        },
        {
          "name": "ZiMuOrder",
          "source_files": "ZiMuSDK/Classes/ZiMuOrder/**/*.{h,m}"
        }
      ]
    }
  ]
}
